<nav>
  <div class='nav-heading'>
    <h5>Course Outline</h5>
  </div>
  <ol class='vertical-left-menu'>
    <% _.each(courses,function(course){ %>
      <li class='sub-menu'><a href= #  class = 'chapters' id = "course_r<%=course.id%>" data-id= <%=course.id%> ><%= course.name %></a>
        <ul>
          <% _.each(course.sections,function(section){ %>
            <li><a href= # id = "section_<%=section.id%>" class = 'sections' data-id= <%=section.id%> " ><%= section.name %></a></li>
          <% }) %>
        </ul>
      </li>
    <% }) %>
  </ol>
</nav>

